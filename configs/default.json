{
    "title": "DEFAULT CONFIG",
    "active": true,
    "TEST_CRON_ONLY": false,
    "FORCE_SCHEDULE_CRON": true,
    "services": {
        "status": {
            "active": true,
            "run_on_start": true,
            "title": "Status Service",
            "cron": "*/10 * * * *",
